<!--Amarsoft Runtime Environmen Config File-->
<CRParseConfig>
	<!--系统属性可以做为多种用途的扩展之用,可以放置任意多的属性在此-->
	<SystemProperties>
	
		<Property name="iCR_dataSource" value="iparse" />
		<Property name="eCR_dataSource" value="eparse" />
		<Property name="vCR_dataSource" value="vparse" />
		
		<Property name="iFilterParseConfig" value="{$ARE.APP_HOME}/etc/ifilter.xml" />
		<Property name="eFilterParseConfig" value="{$ARE.APP_HOME}/etc/efilter.xml" />
		<Property name="LogoPicConfig" value="{$ARE.APP_HOME}/etc/logo.png" />
		
	</SystemProperties>
	
	<ThreadPoolConfig>
		<!-- 核心线程数量 -->
		<Property name="corePoolSize" value="10" />
		<!-- 最大线程数量 -->
		<Property name="maxPoolSize" value="20" />
		<!-- 线程的成活时间  单位 秒 -->
		<Property name="keepAliveTime" value="20" />
		<!-- 等待队列的类型  Array 数组 Linked 链式 -->
		<Property name="workQueueType" value="Linked" />
		<!-- 等待队列的长度 -->
		<Property name="workQueueSize" value="20" />
	</ThreadPoolConfig>
	
	<IDBOperationConfig>
		<!-- 批量提交数量 -->
		<Property name="exeNum" value="2000" />
	</IDBOperationConfig>
	
	<EDBOperationConfig>
		<!-- 批量提交数量 -->
		<Property name="exeNum" value="2000" />
	</EDBOperationConfig>
	
	<IParseHTMLProperties>
		<!-- HTML输出表格大小 -->
		<Property name="width" value="100%" />
		<!-- 字段输出模式 1-代码字段未转换 2-代码字段转换 -->
		<Property name="prType" value="2" />
		<!-- 输出模式 1-输出完整报文结构 2-只输出非空字段的报文 3-浦发个人打印 -->
		<Property name="prMode" value="3" />
	</IParseHTMLProperties>

	<EParseHTMLProperties>
		<!-- HTML输出表格大小 -->
		<Property name="width" value="100%" />
		<!-- 字段输出模式 1-代码字段未转换 2-代码字段转换 -->
		<Property name="prType" value="2" />
		<!-- 输出模式 1-输出完整报文结构 2-只输出非空字段的报文 3-只过滤正文，财务报表，附件三个部分，其他小节点按模式1输出 4-只控制财务报表 -->
		<Property name="prMode" value="4" />
		<!-- 是否显示图片 1-不显示图片 2-显示图片 -->
		<Property name="enPic" value="1" />
	</EParseHTMLProperties>
	
