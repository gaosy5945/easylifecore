	<class name="BILL_IMPORT" label="票据导入" keyAttributes="SERIALNO">
      <attributes>
        <attribute name="SERIALNO" label="流水号" type="STRING" length="16" />
        <attribute name="ISLOCALBILL" label="是否本行号码" type="STRING" length="2" >
        	<extendProperties>
					<property name="excelCol" value="A" />
					<property name="codeMap" value="YesNo" />
			</extendProperties>
        </attribute>
        <attribute name="BILLNO" label="票据号码" type="STRING" length="40" >
        	<extendProperties>
					<property name="excelCol" value="B" />
			</extendProperties>
        </attribute>
         <attribute name="BILLSUM" label="票面金额" type="DOUBLE" length="24" scale="6" >
        	<extendProperties>
						<property name="excelCol" value="C" />
			</extendProperties>
        </attribute>
        <attribute name="WRITEDATE" label="票据签发日期" type="STRING" length="10" >
	        <extendProperties>
							<property name="excelCol" value="D" />
			</extendProperties>
		</attribute>
		<attribute name="MATURITY" label="票据到期日" type="STRING" length="10" >
	        <extendProperties>
							<property name="excelCol" value="E" />
			</extendProperties>
		</attribute>
		<attribute name="FINISHDATE" label="贴现日期" type="STRING" length="10" >
	        <extendProperties>
							<property name="excelCol" value="F" />
			</extendProperties>
		</attribute>
        <attribute name="BEGINDATE" label="票据查询回复日期"  type="STRING" length="10" >
        	<extendProperties>
				<property name="excelCol" value="G" />
			</extendProperties>
        </attribute>
        <attribute name="HOLDERID" label="票据来源"  type="STRING" length="2" >
        	<extendProperties>
				<property name="excelCol" value="H" />
				<property name="codeMap" value="BillResource" />
			</extendProperties>
        </attribute>
        <attribute name="ENDORSETIMES" label="调整天数" type="DOUBLE" length="24" scale="6">
	        <extendProperties>
							<property name="excelCol" value="I" />
			</extendProperties>
		</attribute>
		<attribute name="RATE" label="贴现月利率"  type="DOUBLE" length="24" scale="6">
	        <extendProperties>
							<property name="excelCol" value="J" />
			</extendProperties>
		</attribute>
		<attribute name="AcceptorID" label="承兑行行号" type="STRING" length="32" >
	        <extendProperties>
							<property name="excelCol" value="K" />
			</extendProperties>
		</attribute>
		<attribute name="DEDUCTACCNO" label="放款账号" type="STRING" length="32" >
	        <extendProperties>
							<property name="excelCol" value="L" />
			</extendProperties>
		</attribute>
      </attributes>
      <manager id="MineImport">
        <managerProperties>
          <property name="createKey" value="true" />
          <property name="table" value="BILL_INFO" />
          <property name="startAddress" value="A3" />
		  <property name="finishAddress" value="O10000" />
		  <property name="importClass" value="com.amarsoft.app.als.dataimport.bill.BillExcelImport"/>
		   <property name="excelPath" value="/AppConfig/FileImport/doc/BankAcceptorBill.xls" />
        </managerProperties>
      </manager>
